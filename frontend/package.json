{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:main dev:background",
    "dev:main": "vite build --watch",
    "dev:background": "vite build --watch --config vite.chrome.background.config.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:background:chrome": "vite build --config vite.chrome.background.config.ts",
    "build-all": "run-s build build:background:chrome",
    "tsc": "tsc"
  },
  "dependencies": {
    "@types/axios": "^0.9.36",
    "@types/echarts": "^4.9.22",
    "axios": "^1.13.5",
    "echarts": "^6.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-echarts-library": "^1.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1"
  }
}
